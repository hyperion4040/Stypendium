
@using Stypendium.Controllers
@model IEnumerable<Stypendium.Models.Person>
@{
    var response = 1;
    var res = new StudentController().metoda();
    
}


<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<p></p>
<div>
    <table class="table">
        <tr>
            <th>
                <td>@res[1].NazwaPrzedmiotu</td><td>@res[1].Ocena</td>
            </th>
            <th>
                <td>@res[0].NazwaPrzedmiotu</td><td>@res[0].Ocena</td>
            </th>
            <th>
                <td>@Html.DisplayText("ciekawe")</td><td>@Html.DisplayFor(model => model.GetEnumerator().Current.Id)</td>
            </th>
        </tr>
    </table>
    
    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                
            </tr>
        }

    </table>
</div>
</body>
</html>